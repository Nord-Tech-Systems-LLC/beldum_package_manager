{
    "stevensVectorLib": {
        "git_link": "git@github.com:Bucephalus-Studios/stevensVectorLib.git",
        "description": "A C++ std::vector library that is simple and powerful.",
        "repository_url": "https://github.com/Bucephalus-Studios/stevensVectorLib",
        "repo_type": "header-only",
        "cmake_alias": "none",
        "instructions": {
            "install": [
                "mkdir -p :target_path",
                "git clone :git_link :cache_path",
                "cp -r :cache_path :target_path"
            ],
            "uninstall": ["rm -rf target/debug/deps/:requested_package"],
            "version": [
                "cd :current_path/target/debug/deps/:package_name && git describe --tags --abbrev=0"
            ]
        },
        "tags": ["vectors", "vector", "header-only"]
    }
}
