{
    "nlohmann_json": {
        "git_link": "git@github.com:nlohmann/json.git",
        "description": "JSON for Modern C++.",
        "repository_url": "https://github.com/nlohmann/json",
        "repo_type": "static",
        "cmake_alias": "nlohmann_json::nlohmann_json",
        "instructions": {
            "install": [
                "mkdir -p :target_path",
                "git clone :git_link :cache_path",
                "cp -r :cache_path :target_path"
            ],
            "uninstall": ["rm -rf target/debug/deps/:requested_package"],
            "version": [
                "cd :current_path/target/debug/deps/:package_name && git describe --tags --abbrev=0"
            ]
        },
        "tags": [
            "json",
            "json-serialization",
            "msgpack",
            "cbor",
            "json-parser",
            "header-only",
            "messagepack",
            "json-pointer",
            "json-patch",
            "stl-containers",
            "rfc-6901",
            "rfc-6902",
            "rfc-7159",
            "rfc-7049",
            "json-diff",
            "bson",
            "ubjson",
            "json-merge-patch",
            "rfc-8259"
        ]
    }
}
