{
    "cpp-dotenv": {
        "git_link": "git@github.com:adeharo9/cpp-dotenv.git",
        "description": "Loads environment variables from .env files for C++ projects. ",
        "repository_url": "https://github.com/adeharo9/cpp-dotenv",
        "repo_type": "static",
        "cmake_alias": "cpp_dotenv",
        "instructions": {
            "install": [
                "git clone :git_link :cache_path",
                "cp -r :cache_path :target_path"
            ],
            "uninstall": ["rm -rf target/debug/deps/:requested_package"],
            "version": [
                "cd :current_path/target/debug/deps/:package_name && git describe --tags --abbrev=0"
            ]
        },
        "tags": ["dotenv", "enviornment variables", "dotenv-parser"]
    }
}
