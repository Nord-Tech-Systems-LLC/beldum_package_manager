{
    "stevensStringLib": {
        "git_link": "git@github.com:Bucephalus-Studios/stevensStringLib.git",
        "description": "An easy-to-use library of C++ string functions.",
        "repository_url": "https://github.com/Bucephalus-Studios/stevensStringLib",
        "repo_type": "header-only",
        "cmake_alias": "none",
        "instructions": {
            "install": [
                "git clone :git_link :cache_path",
                "cp -r :cache_path :target_path"
            ],
            "uninstall": ["rm -rf target/debug/deps/:requested_package"],
            "version": [
                "cd :current_path/target/debug/deps/:package_name && git describe --tags --abbrev=0"
            ]
        },
        "tags": ["strings", "header-only"]
    }
}
