{
    "cpp_webserver": {
        "git_link": "git@github.com:Nord-Tech-Systems-LLC/cpp_webserver.git",
        "description": "A C++ webserver for backend development.",
        "repository_url": "https://github.com/Nord-Tech-Systems-LLC/cpp_webserver",
        "repo_type": "static",
        "cmake_alias": "cpp_webserver::cpp_webserver",
        "instructions": {
            "install": [
                "git clone :git_link :cache_path",
                "cp -r :cache_path :target_path"
            ],
            "uninstall": ["rm -rf target/debug/deps/:requested_package"],
            "version": [
                "cd :current_path/target/debug/deps/:package_name && git describe --tags --abbrev=0"
            ]
        },
        "tags": ["webserver", "server"]
    }
}
